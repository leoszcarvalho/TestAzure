@model List<string>
@{
    ViewData["Title"] = "Usuários disponíveis";
}

<h2>Usuários online</h2>

@foreach (var user in Model)
{
    <form asp-action="Start" method="post">
        <input type="hidden" name="otherUser" value="@user" />
        <button type="submit">Conversar com @user</button>
    </form>
}
